
//Create at least three Person records.
//Request person 1
//Request URL: http://localhost:8080/persons/1
//Person 1 body
{
    "created": "2017-12-15T02:21:58.726+0000",
    "name": "Felicia",
    "_links": {
        "self": {
            "href": "http://localhost:8080/persons/1"
        },
        "person": {
            "href": "http://localhost:8080/persons/1"
        },
        "groups": {
            "href": "http://localhost:8080/persons/1/groups"
        },
        "memberships": {
            "href": "http://localhost:8080/persons/1/memberships"
        }
    }
}



